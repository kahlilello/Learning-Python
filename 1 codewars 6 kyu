

/// for CSE 6040 - Finding the odd int

def find_it(seq):
    
    sortedints = sorted(seq)
    current = None
    appearance = 0
    oddnum = None
    
    
    for num in sortedints:
        if current == num:
            appearance += 1
        else:
            if current is None and oddnum is None:
                current = num
                appearance += 1
            if current is not None and appearance % 2 != 0:
                oddnum = current
            else:
                appearance = 1
                current = num
    if current is not None and appearance % 2 != 0:
        oddnum = current
    
    return oddnum
