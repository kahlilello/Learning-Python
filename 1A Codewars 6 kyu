
//// Training on Multoples of 3 or 5
def solution(number):
    i = 0
    multiples = []
    while i < number:
        if i % 3 == 0 or i%5 == 0:
            multiples.append(i)
        i+=1
    
    
    return sum(multiples)







/// for CSE 6040 - Finding the odd int

def find_it(seq):
    
    sortedints = sorted(seq)
    current = None
    appearance = 0
    oddnum = None
    
    
    for num in sortedints:
        if current == num:
            appearance += 1
        else:
            if current is None and oddnum is None:
                current = num
                appearance += 1
            if current is not None and appearance % 2 != 0:
                oddnum = current
            else:
                appearance = 1
                current = num
    if current is not None and appearance % 2 != 0:
        oddnum = current
    
    return oddnum



        XOR Soln:
            def find_it(seq):
                result = 0
                for n in seq:
                    result ^= n
                
                return result
