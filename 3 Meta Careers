# Warmup

# ABC
  def getSum(A: int, B: int, C: int) -> int:
    # Write your code here 
    return A+B+C

# All Wrong
  def getWrongAnswers(N: int, C: str) -> str:
    # Write your code here
    result = ''
    for i in range(N):
      if C[i] == 'A':
        result += 'B'
      else:
        result += 'A'
    return result

    # All Wrong 2
      def getWrongAnswers(N: int, C: str) -> str:
        # Write your code here
        return ''.join('A' if c == 'B' else 'B' for c in C)


