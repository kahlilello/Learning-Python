########### Categorize New Members
def open_or_senior(data):
    
    cat = []
    
    for player in data:
        if player[0] > 54 and player[1] > 7:
            cat.append('Senior')
            
        else:
            cat.append('Open')
            
    return cat



## List Comprehension version
def open_or_senior(data):
    return ['Senior' if player[0] > 54 and player[1] > 7  else 'Open' for player in data]

